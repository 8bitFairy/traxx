d1
$ every 9 ((# s "rave").(# distort 2).(#speed 1.2).(#gain 2))
$ every 8 ((slow 4).(# speed 0.7))
$ every 2 ((# n 2).(slow 2).(stutWith 4 (1/32)(# crush 5) ))
$ every 3 ((# n 1).(fast 2).(loopAt 4))
$ every 5 (stutWith 8 (1/8)(rev).(#gain 2)) 
$ stack[
loopAt 4 $ chop 8 $ n (run "4" ) # s "[bev:2,]" # gain 1.4 # speed "[0 2]/8" # cut 4
,s "[gab:2*4,~ ~ sd ~]" # speed "1" # gain 1.3 # cut 4
] # distort 0.4

setcps (50/60)

hush
